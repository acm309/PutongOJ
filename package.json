{
  "name": "putong-oj",
  "version": "2.0.0-beta",
  "author": "lazzzis",
  "license": "MIT",
  "repository": "https://github.com/acm309/PutongOJ.git",
  "main": "app.js",
  "contributors": [
    {
      "name": "lazzzis",
      "url": "https://github.com/lazzzis"
    },
    {
      "name": "Kerminate",
      "url": "https://github.com/Kerminate"
    }
  ],
  "engines": {
    "node": ">= 9.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon app.js",
    "start": "cross-env NODE_ENV=production node app.js",
    "deploy:init": "cross-env NODE_ENV=production node manage.js",
    "deploy:start": "npx pm2 start pm2.config.json",
    "pretest": "cross-env NODE_ENV=test node test/pretest.js",
    "test": "cross-env NODE_ENV=test node test/pretest.js && nyc ava && node test/posttest.js",
    "report": "nyc report --reporter=html",
    "posttest": "cross-env NODE_ENV=test node test/posttest.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "prepare": "husky install"
  },
  "dependencies": {
    "cross-env": "^5.1.3",
    "dotenv-flow": "^3.2.0",
    "execa": "5.1.1",
    "fs-extra": "^5.0.0",
    "ioredis": "^3.2.2",
    "koa": "^2.4.1",
    "koa-body": "^2.5.0",
    "koa-logger": "^3.1.0",
    "koa-router": "^7.3.0",
    "koa-send": "^4.1.2",
    "koa-session": "^5.5.1",
    "koa-static": "^4.0.2",
    "koa2-ratelimit": "^0.9.0",
    "lodash.difference": "^4.5.0",
    "lodash.pickby": "^4.6.0",
    "lodash.range": "^3.2.0",
    "lodash.remove": "^4.7.0",
    "lodash.without": "^4.4.0",
    "mongoose": "^6.1.3",
    "mongoose-paginate-v2": "^1.6.3",
    "node-fetch": "^2.0.0",
    "nodemailer": "^4.6.4",
    "only": "^0.0.2",
    "pm2": "^5.2.2",
    "tracer": "^0.8.11",
    "url-join": "^4.0.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "https://gitpkg.now.sh/lazzzis/eslint-config/packages/all?my",
    "@antfu/eslint-config-basic": "https://gitpkg.now.sh/lazzzis/eslint-config/packages/basic?my",
    "ava": "^4.3.0",
    "codecov": "^3.0.2",
    "eslint": "^8.16.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^8.0.3",
    "nodemon": "^1.14.3",
    "nyc": "^15.1.0",
    "supertest": "^3.0.0",
    "typescript": "^4.7.4"
  },
  "nodemonConfig": {
    "ignore": [
      "data/*",
      "services/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "pnpm run lint"
    }
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ]
  }
}
